{
  "name": "snipe/snipe-it",
  "description": "Open source asset management system built on Laravel.",
  "keywords": [
    "assets",
    "asset-management",
    "it-tools",
    "inventory",
    "laravel"
  ],
  "license": "AGPL-3.0-or-later",
  "type": "project",
  "require": {
    "php": "^7.2.5",
    "ext-curl": "*",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "adldap2/adldap2": "^10.2",
    "alek13/slack": "^1.12",
    "bacon/bacon-qr-code": "^1.0",
    "barryvdh/laravel-cors": "^0.11.3",
    "barryvdh/laravel-debugbar": "^3.2",
    "eduardokum/laravel-mail-auto-embed": "^1.0",
    "enshrined/svg-sanitize": "^0.13.3",
    "fideloper/proxy": "^4.3",
    "guzzlehttp/guzzle": "^6.5",
    "intervention/image": "^2.5",
    "javiereguiluz/easyslugger": "^1.0",
    "laravel/helpers": "^1.2",
    "laravel/passport": "^9.3.2",
    "laravel/slack-notification-channel": "^2.0",
    "laravel/tinker": "^2.4",
    "laravelcollective/html": "^6.0",
    "league/csv": "^9.5",
    "league/flysystem-aws-s3-v3": "^1.0",
    "league/flysystem-cached-adapter": "^1.0",
    "neitanod/forceutf8": "^2.0",
    "nunomaduro/collision": "^3.2",
    "onelogin/php-saml": "^3.4",
    "paragonie/constant_time_encoding": "^2.3",
    "pragmarx/google2fa-laravel": "^1.3",
    "rollbar/rollbar-laravel": "^6.0",
    "spatie/laravel-backup": "^6.9",
    "symfony/polyfill-mbstring": "^1.3",
    "tecnickcom/tc-lib-barcode": "^1.15",
    "tightenco/ziggy": "^0.9.0",
    "unicodeveloper/laravel-password": "^1.0",
    "watson/validating": "^4.0"
  },
  "require-dev": {
    "codeception/codeception": "^4.1",
    "codeception/module-asserts": "^1.2",
    "codeception/module-laravel5": "^1.1",
    "codeception/module-rest": "^1.2",
    "codeception/module-webdriver": "^1.0",
    "fakerphp/faker": "^1.9",
    "overtrue/phplint": "^2.2",
    "phpunit/php-token-stream": "^3.1",
    "phpunit/phpunit": "^8.5",
    "squizlabs/php_codesniffer": "^3.5",
    "symfony/css-selector": "^4.4",
    "symfony/dom-crawler": "^4.4"
  },
  "suggest": {
    "ext-mcrypt": "For upgrading from before v5.1 this is used to re-encrypt data"
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "rollbar/rollbar-laravel"
      ]
    }
  },
  "autoload": {
    "classmap": [
      "/usr/share/php/Carbon",
      "/usr/share/php/PHP/CodeSniffer",
      "/usr/share/php/Cron",
      "/usr/share/php/Defuse",
      "/usr/share/php/Defuse/Crypto",
      "/usr/share/php/Doctrine",
      "/usr/share/php/Dotenv",
      "/usr/share/php/Egulias/EmailValidator",
      "/usr/share/php/getallheaders",
      "/usr/share/php/GuzzleHttp",
      "/usr/share/php/GuzzleHttp/Promise",
      "/usr/share/php/GuzzleHttp/Psr7",
      "/usr/share/php/Illuminate",
      "/usr/share/php/Illuminate/Support",
      "/usr/share/php/Illuminate/Notifications/Channels",
      "/usr/share/php/Http/Message",
      "/usr/share/php/Illuminate/Foundation",
      "/usr/share/php/Illuminate/Foundation/helpers.php",
      "/usr/share/php/League",
      "/usr/share/php/League/CommonMark",
      "/usr/share/php/League/Flysystem",
      "/usr/share/php/League/MimeTypeDetection",
      "/usr/share/php/Masterminds",
      "/usr/share/php/Masterminds/HTML5",
      "/usr/share/php/Mockery",
      "/usr/share/php/Monolog",
      "/usr/share/php/Nyholm/Psr7",
      "/usr/share/php/Opis/Closure",
      "/usr/share/php/Parsedown",
      "/usr/share/php/PHP/CodeSniffer",
      "/usr/share/php/phpDocumentor",
      "/usr/share/php/PhpOption",
      "/usr/share/php/PhpParser",
      "/usr/share/php/phpseclib",
      "/usr/share/php/PHPUnit",
      "/usr/share/php/Prophecy",
      "/usr/share/php/Psr",
      "/usr/share/php/Psr/Cache",
      "/usr/share/php/Psr/Http",
      "/usr/share/php/Psr/Http/Message",
      "/usr/share/php/Psr/Log",
      "/usr/share/php/Psr/SimpleCache",
      "/usr/share/php/Ramsey/Uuid",
      "/usr/share/php/random_compat",
      "/usr/share/php/SebastianBergmann/Version",
      "/usr/share/php/Symfony",
      "/usr/share/php/Symfony/Component/HttpFoundation",
      "/usr/share/php/Symfony/Component/CssSelector",
      "/usr/share/php/Symfony/Component/Translation",
      "/usr/share/php/Symfony/Component/Mailer",
      "/usr/share/php/Symfony/Component/Mime",
      "/usr/share/php/Symfony/Component/Process",
      "/usr/share/php/Symfony/Component/Routing",
      "/usr/share/php/Symfony/Component/VarDumper",
      "/usr/share/php/Symfony/Contracts",
      "/usr/share/php/Symfony/Polyfill",
      "/usr/share/php/Symfony/Polyfill/Ctype",
      "/usr/share/php/Symfony/Polyfill/Intl/Idn",
      "/usr/share/php/Symfony/Polyfill/Intl/Normalizer",
      "/usr/share/php/TijsVerkoyen/CssToInlineStyles",
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php",
      "tests/unit/BaseTest.php"
    ]
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "discard-changes": true,
    "process-timeout": 3000,
    "platform": {
      "php": "7.2.5"
  }
  }
}
